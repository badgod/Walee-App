<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.character">

    <application
        android:name="com.example.character.Application"
        android:allowBackup="true"
        android:icon="@drawable/icon_walee"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_walee"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.character.AddWaleeActivity"></activity>
        <activity android:name="com.example.character.ShowAllListActivity"></activity>
        <activity android:name="com.example.character.EditWaleeActivity"></activity>
        <activity android:name="com.example.character.RandomWaleeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>